{
  "name": "appbase-ce-app-init",
  "version": "0.0.1",
  "description": "Setup logic for AppBase CE services",
  "author": "AppBase AI, Inc.",
  "homepage": "https://www.appbase.ai",
  "license": "UNLICENSED",
  "repository": "https://github.com/appbase-ai/appbase-community-edition",
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules out",
    "fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts' && npm run lint -- --fix",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prepare": "npm run build",
    "test": "node --no-experimental-fetch node_modules/.bin/jest --verbose --color",
    "test:integration": "node --no-experimental-fetch node_modules/.bin/jest test/integration-tests/*.ts --verbose --color",
    "test:unit": "node --no-experimental-fetch node_modules/.bin/jest test/unit-tests/**/*.ts --verbose --color",
    "test-cov": "node --no-experimental-fetch node_modules/.bin/jest --coverage --verbose --color",
    "watch": "tsc -b -w src test"
  },
  "files": [
    "lib/",
    "resources/"
  ],
  "dependencies": {
    "@segment/analytics-node": "^2.3.0",
    "async-retry": "^1.3.3",
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "figlet": "^1.9.4",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.3.2",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.21",
    "p-limit": "^7.2.0",
    "pino": "^10.1.0",
    "pluralize": "^8.0.0",
    "tar": "^7.5.2",
    "uuid": "^13.0.0",
    "verror": "^1.10.1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/pluralize": "^0.0.33",
    "@types/tar": "^6.1.13",
    "@types/uuid": "^10.0.0",
    "@types/verror": "^1.10.11",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.2.0",
    "nock": "^14.0.10",
    "pino-pretty": "^13.1.2",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "bracketSpacing": false,
    "parser": "typescript",
    "trailingComma": "es5",
    "singleQuote": true
  },
  "jest": {
    "coverageDirectory": "out/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "testTimeout": 5000,
    "transform": {
      "^.+\\.ts$": [
        "ts-jest", {
          "tsconfig": "test/tsconfig.json"
        }
      ]
    }
  },
  "overrides": {
    "cross-spawn": "^7.0.5",
    "js-yaml": "^4.1.1",
    "glob": "^11.1.0",
    "tar": "^7.5.2"
  }
}
